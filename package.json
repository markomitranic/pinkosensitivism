{
  "name": "pinko",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "pnpm nodecheck && astro dev --port 3000",
    "build": "pnpm nodecheck && astro build --remote",
    "preview": "astro preview",
    "start": "astro preview",
    "lint": "pnpm lint:style && pnpm lint:code && pnpm lint:typecheck",
    "lint:code": "npx eslint \"src/**/*.{js,astro,ts}\"",
    "lint:style": "npx prettier -c .",
    "lint:typecheck": "tsc --noEmit",
    "nodecheck": "CODE_NODE_VERSION=\"v$(cat .nvmrc)\" && NODE_VERSION=$(node --version) && if [ $NODE_VERSION != $CODE_NODE_VERSION ]; then echo \"ERROR: Project Node: $CODE_NODE_VERSION, Actual Node: $NODE_VERSION\n\" && exit 1; fi",
    "astro": "astro",
    "husky": "./.husky/pre-push",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/db": "^0.14.10",
    "@astrojs/vercel": "^8.1.3",
    "@aws-sdk/client-s3": "^3.782.0",
    "@fontsource/roboto": "^5.2.5",
    "@tailwindcss/vite": "^4.1.3",
    "@unpic/astro": "^1.0.0",
    "@unpic/pixels": "^1.3.0",
    "@unpic/placeholder": "^0.1.2",
    "@vercel/analytics": "^1.5.0",
    "astro": "^5.6.1",
    "blurhash": "^2.0.5",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "sharp": "^0.34.0",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.3",
    "unpic": "^4.1.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.0",
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@typescript-eslint/parser": "^8.29.0",
    "eslint": "^9.24.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "prettier": "3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.0"
  },
  "packageManager": "pnpm@10.7.1"
}
