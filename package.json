{
  "name": "pinko",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --port 3000",
    "build": "astro build",
    "preview": "astro preview",
    "start": "astro preview",
    "lint": "npm run lint:code && npm run lint:style && npm run lint:typecheck",
    "lint:code": "npx eslint \"src/**/*.{js,astro,tsx,ts}\"",
    "lint:style": "npx prettier -c .",
    "lint:typecheck": "tsc --noEmit",
    "nodecheck": "CODE_NODE_VERSION=\"v$(cat .nvmrc)\" && NODE_VERSION=$(node --version) && if [ $NODE_VERSION != $CODE_NODE_VERSION ]; then echo \"ERROR: Project Node: $CODE_NODE_VERSION, Actual Node: $NODE_VERSION\n\" && exit 1; fi",
    "astro": "astro",
    "husky": "./.husky/pre-push",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/react": "^4.2.0",
    "@astrojs/solid-js": "^5.0.4",
    "@astrojs/vercel": "^8.0.6",
    "@fontsource/roboto": "^5.1.1",
    "@tailwindcss/vite": "^4.0.6",
    "@uiw/react-json-view": "2.0.0-alpha.30",
    "astro": "^5.2.5",
    "lodash-es": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "solid-js": "^1.9.4",
    "tailwind-merge": "^3.0.1",
    "tailwindcss": "^4.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "husky": "^9.1.7",
    "prettier": "3.5.0",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-tailwindcss": "0.6.11",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
